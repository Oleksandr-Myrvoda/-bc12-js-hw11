{{#each this}}
  <div class='photo-card'>
    <a class='img-link' href={{largeImageURL}}><img
        class='img-item'
        src={{webformatURL}}
        alt={{tags}}
        loading='lazy'
      />
    </a>
    <div class='info'>
      <p class='info-item'>
        <b class='info-name'>Likes</b>
        {{likes}}
      </p>
      <p class='info-item'>
        <b class='info-name'>Views</b>
        {{views}}
      </p>
      <p class='info-item'>
        <b class='info-name'>Comments</b>
        {{comments}}
      </p>
      <p class='info-item'>
        <b class='info-name'>Downloads</b>
        {{downloads}}
      </p>
    </div>
  </div>
{{/each}}